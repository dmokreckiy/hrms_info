<h2> Pages:List</h2>
<table width="70%" border="2" cellspacing="0" cellpadding="3" background="#c0c0c0">
	
	<tr align= "left">
		<th> Title</th>
		<th> Published </th>
		<th> Last changed </th>
	</tr>
	
	<% @pages.each do |i| %>
	
	<tr align="left" valign= "">
		<td width ="50%"><%= i.page_title %> </td>
		<td width ="20%" align="center"><%= check_box_tag :publsihed, true, false %></td>
		<td width ="30%"><%= i.updated_at.strftime("%d/%m/%y %l:%M %p")  %> </td>
	</tr>

<% end %>

</table>




<div class="pagination" id="pagination">
	

</div>
